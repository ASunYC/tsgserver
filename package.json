{
  "name": "@asun01/tsgserver",
  "version": "1.0.0",
  "description": "A TypeScript Lib For HTTP and WebSocket.",
  "main": "index.js",
  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "typings": "index.d.ts",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "npx mocha",
    "coverage": "nyc mocha test/**/*.test.ts && start coverage\\index.html",
    "build": "npm run clean && npm run build:js && npm run build:dts && npm run build:doc && node scripts/postBuild && shx cp package.json LICENSE README.md dist/",
    "clean": "shx rm -rf dist && shx rm -rf lib && shx rm -rf docs/api",
    "build:js": "npx rollup -c",
    "build:dts": "npx tsc && npx api-extractor run --local --verbose",
    "build:doc": "npx api-documenter markdown --input temp --output docs/api",
    "login": "npm login --registry https://registry.npmjs.org",
    "publish": "cd dist && npm publish --registry https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ASunYC/tsgserver.git"
  },
  "keywords": [
    "asun01",
    "ts",
    "rpc",
    "grpc",
    "tsbuffer",
    "fullstack",
    "websocket",
    "protobuf",
    "socket.io"
  ],
  "author": "asun01",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.24.2",
    "@microsoft/api-extractor": "^7.43.1",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^8.2.3",
    "@types/node": "^15.14.9",
    "@types/uuid": "^8.3.4",
    "chai": "^4.4.1",
    "mocha": "^9.2.2",
    "mongodb": "^4.17.2",
    "nyc": "^15.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/ws": "^7.4.7",
    "chalk": "^4.1.2",
    "uuid": "^8.3.2",
    "tslib": "*",
    "ws": "^7.5.9"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}